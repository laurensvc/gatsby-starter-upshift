{
  "name": "gatsby-starter-upshift",
  "private": true,
  "description": "A simplified startup website using gatsby, netlify and more. Maintained by upshift.",
  "version": "0.1.1",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.21.21",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.3.1",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^2.4.2",
    "gatsby-plugin-netlify-cms": "^4.3.1",
    "gatsby-plugin-offline": "^3.2.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-plugin-sitemap": "^2.4.2",
    "gatsby-plugin-styled-components": "^3.3.1",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-remark": "^2.8.7",
    "gatsby-transformer-sharp": "^2.5.2",
    "netlify-cms-app": "^2.12.12",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "reflexbox": "^4.0.6",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "babel-preset-gatsby": "^0.4.1",
    "eslint": "^7.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "lint-staged": {
    "src/**/*.js*": [
      "prettier --trailing-comma es5 --single-quote --print-width 120 --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laurensvc/gatsby-starter-upshift"
  }
}
