{
  "name": "gatsby-starter-upshift",
  "private": true,
  "description": "A simplified startup website using gatsby, netlify and more. Maintained by upshift.",
  "version": "0.1.1",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "gatsby": "^2.18.4",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.1.29",
    "gatsby-plugin-manifest": "^2.2.30",
    "gatsby-plugin-netlify-cms": "^4.1.30",
    "gatsby-plugin-offline": "^3.0.25",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.3.4",
    "gatsby-plugin-sitemap": "^2.2.22",
    "gatsby-plugin-styled-components": "^3.1.14",
    "gatsby-source-filesystem": "^2.1.39",
    "gatsby-transformer-remark": "^2.6.38",
    "gatsby-transformer-sharp": "^2.3.6",
    "netlify-cms-app": "^2.9.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "reflexbox": "^4.0.6",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "eslint": "^6.7.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "lint-staged": {
    "src/**/*.js*": [
      "prettier --trailing-comma es5 --single-quote --print-width 120 --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laurensvc/gatsby-starter-upshift"
  }
}
